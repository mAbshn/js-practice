// Task: write a function that clears an array of unwanted values such as false, undefined, empty strings, 0, null

const compact = (array) => {
        newArray = array.filter(elem => elem);
        return newArray;
}

const data = [0, 1, false, 2, undefined, '', 3, null];
console.log(compact(data)) // [1, 2, 3]
